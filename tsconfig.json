{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "paths": {
      "@/*": ["./*"],
      "@/lib/webviewer-types": ["./registry/lib/webviewer-types.ts"],
      "@/context/webviewer-context": ["./registry/context/webviewer-context.tsx"],
      "@/hooks/*": ["./registry/hooks/*"],
      "@/components/viewer-element": ["./registry/components/viewer-element.tsx"],
      "@/components/scroll-view-container": ["./registry/components/scroll-view-container.tsx"],
      "@/components/viewer-canvas": ["./registry/components/viewer-canvas.tsx"],
      "@/components/document": ["./registry/components/document.tsx"],
      "fumadocs-mdx:collections/*": [".source/*"],
      "@pdftron/webviewer": ["./types/webviewer.d.ts"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "types/**/*.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": ["node_modules", "components/webviewer-legacy"]
}
